{
  "manifest_version": 3,
  "name": "Google Chrome Built-in AI Challenge",
  "version": "1.0",
  "description": "Develop a web application that uses one or more Chrome built-in AI APIs to interact with integrated models such as Gemini Nano.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
